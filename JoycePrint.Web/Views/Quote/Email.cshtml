@model JoycePrint.Domain.Models.QuoteRequest

@{
    ViewBag.Title = "Trade Docket Books";
    Layout = "~/Views/Shared/_Layout.cshtml";

    BundleConfig.UpdateScriptBundle(BundleTable.Bundles.GetBundleFor("~/js/joyceprintjs"), BundleConfig.PageBundle.All);
}


<div class="container">
    <div class="row">
        <div class="col s6">

            <h4 class="primary-text">Name</h4> : @Model.Contact.Name
            <h4 class="primary-text">Phone</h4> : @Model.Contact.Phone
            <h4 class="primary-text">Email</h4> : @Model.Contact.Email
            <h4 class="primary-text">Company</h4> : @Model.Contact.Company

        </div>

        <div class="col s6">

            <h4 class="primary-text">Message</h4>
            <p>Model.Enquiry.Message</p>

        </div>
    </div>
</div>
