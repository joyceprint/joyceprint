<!-- ######################### Windows 8 IE10 ######################### -->

<!-- application-name - The name of the application if pinned to the start screen. -->
<meta name="application-name" content="JoycePrint">

<!-- ######################### Windows 8.1 IE11 ######################### -->

<!-- msapplication-TileColor - The tile colour which shows around your tile image (msapplication-TileImage). -->
<meta name="msapplication-TileColor" content="#FFFFFF" />

<!-- msapplication-TileImage - The tile image. -->
<meta name="msapplication-TileImage" content="@Url.Content("~/Content/favicon/favicon-144.png")" />

<!--
    msapplication-config - The location of the browser configuration file.
    If you have an RSS feed, go to http://www.buildmypinnedsite.com and regenerate the browserconfig.xml file.
    You will then have a cool live tile!
-->
<meta name="msapplication-config" content="@Url.Content("~/Content/favicon/browserconfig.xml")" />