@{
    ViewBag.Title = "Quote";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section id="quote" class="col-xs-12">
    <div class="text-center jp-delivery">
        <h1 class="text-danger text-weight-900">
            Delivery to Ireland & The UK
        </h1>
    </div>

    <div class="row row-vertical-align">
        <div class="col-sm-6 col-md-5 col-md-offset-1 col-lg-4 col-lg-offset-1">
            @{
                Html.RenderPartial("DocketHelp");
            }
        </div>

        <div class="col-sm-6 col-md-6 col-lg-6">
            @{
                Html.RenderPartial("DocketForm");
            }
        </div>
    </div>
</section>